{
	"name": "@specialdoom/proi-ui",
	"version": "3.2.0",
	"description": "yet another Svelte UI lib",
	"keywords": [
		"svelte",
		"ui",
		"svelte ui library",
		"components",
		"svelte3"
	],
	"author": "specialdoom (https://github.com/specialdoom)",
	"repository": "git://github.com/specialdoom/proi-ui",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"homepage": "https://specialdoom.github.io/proi-ui/",
	"bugs": {
		"url": "https://github.com/specialdoom/proi-ui/issues"
	},
	"scripts": {
		"dev": "histoire dev",
		"build": "node ./tasks/build.js",
		"build:dev": "node ./tasks/build.js --no-release",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write ./lib/**/*.{js,svelte,html,ts}",
		"lint": "eslint ./lib/**/*.{js,ts,svelte}",
		"prelint": "npm run format",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"story:build": "histoire build",
		"story:preview": "histoire preview",
		"prepare": "husky install",
		"release": "standard-version",
		"publish": "node ./tasks/publish.js"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@histoire/plugin-svelte": "^0.11.5",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"histoire": "^0.11.5",
		"husky": "^8.0.2",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"standard-version": "^9.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.0"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": [
			"svelte-check",
			"npm run lint"
		]
	},
	"type": "module"
}