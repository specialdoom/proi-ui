{
	"name": "@specialdoom/proi-ui",
	"version": "3.0.5-alpha.4",
	"description": "yet another Svelte UI lib",
	"keywords": [
		"svelte",
		"ui",
		"svelte ui library",
		"components",
		"svelte3"
	],
	"author": "specialdoom (https://github.com/specialdoom)",
	"repository": "git://github.com/specialdoom/proi-ui",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"homepage": "https://specialdoom.github.io/proi-ui/",
	"bugs": {
		"url": "https://github.com/specialdoom/proi-ui/issues"
	},
	"scripts": {
		"dev": "svelte-kit sync && svelte-package --watch",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"dev:pack": "npm run build && cd package && npm pack",
		"test": "jest",
		"test:watch": "npm run test -- --watchAll",
		"test:coverage": "jest --coverage"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.1",
		"@types/jest": "^29.0.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jest": "^28.1.3",
		"jest-environment-jsdom": "^29.0.2",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.6",
		"ts-jest": "^28.0.8",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.0"
	},
	"type": "module"
}
